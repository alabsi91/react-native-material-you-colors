{
  "name": "react-native-material-you-colors",
  "version": "0.1.0",
  "description": "Get and generate Material You colors",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "cpp",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepack": "bob build",
    "build:android": "cd example/android && .\\gradlew assembleDebug",
    "clean": "del-cli android/build example/android/build example/android/app/build"
  },
  "keywords": [
    "react-native",
    "palette",
    "colors",
    "material you",
    "material design",
    "dynamic color",
    "android"
  ],
  "repository": "https://github.com/alabsi91/react-native-material-you-colors",
  "author": "alabsi91 <alabsi91@gmail.com> (https://github.com/alabsi91)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alabsi91/react-native-material-you-colors/issues"
  },
  "homepage": "https://github.com/alabsi91/react-native-material-you-colors#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.2",
    "@types/react": "~17.0.21",
    "@types/react-native": "0.70.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.0.5",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-builder-bob": "^0.20.0",
    "turbo": "^1.10.7",
    "typescript": "^5.0.2"
  },
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "curly": 0,
      "@typescript-eslint/consistent-type-imports": 1,
      "@typescript-eslint/no-non-null-assertion": 0,
      "react-native/no-unused-styles": 1,
      "react-native/no-inline-styles": 0,
      "react-native/no-single-element-style-arrays": 1,
      "react-native/no-raw-text": 2,
      "react-hooks/exhaustive-deps": 0,
      "object-shorthand": 1,
      "require-await": 1,
      "prettier/prettier": 1,
      "no-shadow": 0,
      "no-bitwise": 0,
      "jsx-quotes": 0
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/",
    "scripts/"
  ],
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 130,
    "jsxSingleQuote": true,
    "semi": true,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "endOfLine": "auto",
    "singleQuote": true
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "tsc": "node_modules/typescript/bin/tsc"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "RNMaterialYouColorsSpec",
    "type": "modules",
    "jsSrcsDir": "",
    "android": {
      "javaPackageName": "com.materialyoucolors"
    }
  }
}
